{
  "name": "pbweb/buzz-bundle",
  "type": "symfony-bundle",
  "description": "PBWeb buzz bundle for Symfony",
  "keywords": ["buzz", "http", "http client", "bundle", "symfony"],
  "license": "MIT",
  "require": {
    "php": "^5.5|^7.0",
    "kriswallsmith/buzz": "~0.13",
    "symfony/config": "^2.3|^3.0",
    "symfony/dependency-injection": "^2.3|^3.0",
    "symfony/http-kernel": "^2.3|^3.0",
    "symfony/stopwatch": "^2.3|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "sebastian/phpcpd": "^2.0",
    "phploc/phploc": "^3.0",
    "nicofuma/php-codebrowser": "^1.1"
  },
  "replace": {
    "sensio/buzz-bundle": "*"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\BuzzBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pbweb\\BuzzBundle\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
