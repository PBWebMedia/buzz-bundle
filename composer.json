{
  "name": "pbweb/buzz-bundle",
  "type": "symfony-bundle",
  "description": "PB Web Media Buzz bundle for Symfony",
  "keywords": ["buzz", "http", "http client", "bundle", "symfony"],
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "kriswallsmith/buzz": "~0.16",
    "symfony/config": "^3.4|^4.0",
    "symfony/dependency-injection": "^3.4|^4.0",
    "symfony/http-kernel": "^3.4|^4.0",
    "symfony/stopwatch": "^3.4|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "pbweb/pbweb-coding-standard": "^1.1.1",
    "sebastian/phpcpd": "^2.0",
    "phploc/phploc": "^3.0",
    "nicofuma/php-codebrowser": "^1.1"
  },
  "replace": {
    "sensio/buzz-bundle": "*"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\BuzzBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pbweb\\BuzzBundle\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.3.x-dev"
    }
  }
}
