{
  "name": "pbweb/buzz-bundle",
  "type": "symfony-bundle",
  "description": "PBWeb buzz bundle for Symfony2",
  "keywords": ["pbweb", "buzz", "http", "bundle", "symfony2"],
  "license": "proprietary",
  "require": {
    "php": ">=5.5",
    "kriswallsmith/buzz": "~0.13",
    "symfony/config": "~2.3",
    "symfony/dependency-injection": "~2.3",
    "symfony/http-kernel": "~2.3",
    "symfony/stopwatch": "~2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "sebastian/phpcpd": "~2.0",
    "phploc/phploc": "~2.0",
    "mayflower/php-codebrowser": "~1.1"
  },
  "replace": {
    "sensio/buzz-bundle": "*"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\BuzzBundle\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
